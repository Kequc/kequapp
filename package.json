{
    "name": "kequapp",
    "version": "0.9.1",
    "description": "A minimal, zero-magic Node web framework built on native APIs",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/main.d.ts",
            "import": "./dist/main.js"
        },
        "./package.json": "./package.json"
    },
    "engines": {
        "node": ">=20.10"
    },
    "sideEffects": false,
    "scripts": {
        "build": "rm -rf dist && tsc",
        "prepare": "npm run build",
        "start:basic": "node ./examples/basic/server.ts",
        "start:file-server": "node ./examples/file-server/server.ts",
        "test": "node --test"
    },
    "files": [
        "dist",
        "changelog.md",
        "LICENSE",
        "readme.md"
    ],
    "bugs": {
        "url": "https://github.com/Kequc/kequapp/issues"
    },
    "homepage": "https://kequapp.kequtech.com",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/Kequc/kequapp.git"
    },
    "author": "Nathan Lunde-Berry",
    "license": "ISC",
    "keywords": [
        "createApp",
        "IncomingMessage",
        "ServerResponse",
        "web",
        "framework",
        "http",
        "rest",
        "server",
        "api"
    ],
    "devDependencies": {
        "@biomejs/biome": "^2.1.1",
        "@types/node": "^24.0.14",
        "typescript": "^5.8.3"
    }
}
